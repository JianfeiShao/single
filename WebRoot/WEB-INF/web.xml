<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<servlet>
		<servlet-name>UserLogin</servlet-name>
		<servlet-class>com.zht.train.servlet.UserLoginServlet</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>QueryServlet</servlet-name>
    <servlet-class>com.zht.train.servlet.QueryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SubmitServlet</servlet-name>
    <servlet-class>com.zht.train.servlet.SubmitServlet</servlet-class>
  </servlet>



	<servlet-mapping>
		<servlet-name>UserLogin</servlet-name>
		<url-pattern>/UserLogin</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryServlet</servlet-name>
    <url-pattern>/QueryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SubmitServlet</servlet-name>
    <url-pattern>/SubmitServlet</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
